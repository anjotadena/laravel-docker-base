{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel Base API Documentation",
        "description": "Coll RESTFul API for laravel base",
        "contact": {
            "email": "tadena.anjo@gmail.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0.3"
    },
    "servers": [
        {
            "url": "http://laravel-base.test",
            "description": "Larabel Base"
        }
    ],
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "User Authentication"
                ],
                "summary": "Login registered user",
                "description": "Login registered user",
                "operationId": "userLogin",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginResource"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/logout": {
            "post": {
                "tags": [
                    "User Authentication"
                ],
                "summary": "Logout User",
                "description": "Logout user here",
                "operationId": "Logout User",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Register Successfully",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized user",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/register": {
            "post": {
                "tags": [
                    "User Authentication"
                ],
                "summary": "Register User",
                "description": "Register user here",
                "operationId": "Register User",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Register Successfully",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "404": {
                        "description": "Resource Not Found"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "LoginRequest": {
                "title": "Login request",
                "description": "Login request payload or json data",
                "required": [
                    "password"
                ],
                "properties": {
                    "email": {
                        "title": "email",
                        "description": "User email",
                        "type": "string",
                        "example": "test@gmail.com"
                    },
                    "password": {
                        "title": "password",
                        "description": "User password",
                        "type": "string"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "LoginRequest"
                }
            },
            "LoginResource": {
                "title": "LoginResource",
                "description": "Login resource",
                "properties": {
                    "token": {
                        "title": "token",
                        "description": "User auth token"
                    },
                    "name": {
                        "title": "name",
                        "description": "User name"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "LoginResource"
                }
            },
            "RegisterResource": {
                "title": "RegisterResource",
                "description": "Register resource",
                "properties": {
                    "message": {
                        "title": "message",
                        "description": "Message"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "RegisterResource"
                }
            }
        },
        "securitySchemes": {
            "sanctum": {
                "type": "apiKey",
                "description": "Enter token in format (Bearer <token>)",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "Laravel Base",
            "description": "API Endpoints of Laravel Base"
        }
    ],
    "security": [
        []
    ]
}